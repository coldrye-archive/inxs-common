{
    "source": "./src",
    "destination": "build/doc",
    "includes": ["\\.es$"],
    "access": ["public", "protected"],
    "autoPrivate": true,
    "unexportIdentifier": false,
    "undocumentIdentifier": true,
    "builtinExternal": true,
    "index": "./README.md",
    "package": "./package.json",
    "coverage": true,
    "includeSource": true,
    "test": {
        "type": "mocha",
        "source": "./test",
        "includes": ["\\.es$"]
    }
}
